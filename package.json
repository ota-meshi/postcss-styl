{
  "name": "stylus-postcss",
  "version": "0.0.1",
  "description": "PostCSS parser plugin for converting Stylus syntax to PostCSS nodes.",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test:base": "mocha \"tests/**/*.js\" --reporter dot",
    "test:update-fixtures": "cross-env UPDATE_FIXTURES=true npm run test:base",
    "test": "npm run test:nyc",
    "test:nyc": "nyc --reporter=lcov npm run test:base -- --timeout 60000",
    "test:d": "mocha --inspect --debug-brk \"tests/**/*.js\" --reporter dot"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ota-meshi/stylus-postcss.git"
  },
  "keywords": [
    "postcss",
    "stylus",
    "parser",
    "css"
  ],
  "author": "Yosuke Ota",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ota-meshi/stylus-postcss/issues"
  },
  "homepage": "https://github.com/ota-meshi/stylus-postcss#readme",
  "engines": {
    "node": "^8.10.0 || ^10.13.0 || >=11.10.1"
  },
  "files": [
    "lib"
  ],
  "devDependencies": {
    "@mysticatea/eslint-plugin": "^10.0.3",
    "autoprefixer": "^9.6.1",
    "cross-env": "^5.2.0",
    "eslint": "^6.0.1",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "postcss-parser-tests": "^6.5.0"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "fast-diff": "^1.2.0",
    "lodash.sortedlastindex": "^4.1.0",
    "postcss": "^7.0.17",
    "semver": "^6.2.0",
    "stylus": "^0.54.5"
  }
}
